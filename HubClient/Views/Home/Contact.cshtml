@{
    ViewBag.Title = "Контакты";
}
<body onload="initializeContactCharts()">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h2>
                @ViewBag.Title 
                <a class="btn btn-primary" style="border:none; " role="button" data-toggle="collapse" href="#collapseAddress" aria-expanded="false" aria-controls="collapseAddress">
                    <span class="glyphicon glyphicon-triangle-bottom"></span>
                </a>
            </h2>
            <div class="collapse" id="collapseAddress">
